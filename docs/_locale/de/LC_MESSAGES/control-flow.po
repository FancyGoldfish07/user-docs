#: ../../control-flow/parallel-gateway.rst:83
#: ../../control-flow/parallel-gateway.rst:77
#: ../../control-flow/parallel-gateway.rst:83
msgid "Undesirable loopback"
msgstr ""

#: ../../control-flow/parallel-gateway.rst:79
#: ../../control-flow/parallel-gateway.rst:79
msgid "and this:"
msgstr ""

